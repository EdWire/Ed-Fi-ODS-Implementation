  <xs:annotation>
    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentClassGroupAssociation">
    <xs:annotation>
      <xs:documentation>This entity tracks student enrollment in class/groups at community providers.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Student associated with the Class Group.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ClassGroupReference" type="EXTENSION-ClassGroupReferenceType">
            <xs:annotation>
              <xs:documentation>Class Group  associated with the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BeginDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The month, day, and year on which the student began participating in a class/group.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The month, day, and year on which the student stopped participating in a class/group.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReasonExited" type="ReasonExitedDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The reason the student stopped participating in a class/group.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>ReasonExitedDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentCommunityProviderLocationAssociation">
    <xs:annotation>
      <xs:documentation>This entity tracks student attendance events such as absent, tardy, etc. at a community provider location level.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>Student associated with the Community Provider Location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CommunityProviderLocationReference" type="EXTENSION-CommunityProviderLocationReferenceType">
            <xs:annotation>
              <xs:documentation>Community Provider Location associated with the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BeginDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The month, day, and year on which the student began participating at a provider location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The month, day, and year on which the student stopped participating at a provider location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReasonExited" type="ReasonExitedDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The reason the child left the community provider.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>ReasonExitedDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EnrollmentStatus" type="EXTENSION-EnrollmentStatus" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The status of the student's enrollment by the Community Provider Loaction.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
