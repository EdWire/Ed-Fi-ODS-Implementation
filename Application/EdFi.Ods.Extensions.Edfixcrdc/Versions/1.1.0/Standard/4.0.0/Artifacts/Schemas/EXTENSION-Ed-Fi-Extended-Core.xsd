              <xs:appinfo>
                <ann:Descriptor>EXTENSION-RestraintCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SchoolExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="School">
        <xs:sequence>
          <xs:element name="AlternativeStatus" type="EXTENSION-AlternativeStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The alternative status of the school, such as academic or discipline</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-AlternativeStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="UngradedDetail" type="EXTENSION-UngradedDetailDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Description of the population of students who attend an ungraded school</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-UngradedDetailDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolTechnology" type="EXTENSION-SchoolTechnology" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Technology related information needed for CRDC at school level.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Descriptors =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AlternativeStatusDescriptor">
    <xs:annotation>
      <xs:documentation>The alternative status of the school, such as academic or discipline.</xs:documentation>
