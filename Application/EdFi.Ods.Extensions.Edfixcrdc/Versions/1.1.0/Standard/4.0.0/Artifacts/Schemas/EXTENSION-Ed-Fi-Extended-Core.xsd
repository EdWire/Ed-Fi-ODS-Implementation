  <xs:annotation>
    <xs:documentation>===== Common Types =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-ClassGroupMeetingTime">
    <xs:annotation>
      <xs:documentation>The times and days of week this class/group is in session.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Common</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MeetingDay" type="EXTENSION-MeetingDayDescriptorReferenceType">
        <xs:annotation>
          <xs:documentation>Indicates meeting day.</xs:documentation>
          <xs:appinfo>
            <ann:Descriptor>EXTENSION-MeetingDayDescriptor</ann:Descriptor>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="StartTime" type="xs:time">
        <xs:annotation>
          <xs:documentation>The time of day a class/group starts providing services.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StopTime" type="xs:time">
        <xs:annotation>
          <xs:documentation>The time of day a class/group stops providing services.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CommunityProvderLicense">
    <xs:annotation>
      <xs:documentation>Licensing information for the community provider.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Common</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LicensingOrganization" type="LicensingOrganization">
        <xs:annotation>
          <xs:documentation>The name of the licensing organization.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LicenseEffectiveDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>The date this license became effective.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LicenseExpiratonDate" type="xs:date">
        <xs:annotation>
          <xs:documentation>The date in which this license expires.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InitialLicenseDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The first date a license of the given type was assigned to the community provider.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LicenseStatus" type="LicenseStatusDescriptorReferenceType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Indicates the status of the community provider's license.</xs:documentation>
          <xs:appinfo>
            <ann:Descriptor>LicenseStatusDescriptor</ann:Descriptor>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfFatalities" type="EXTENSION-NumberOfFatalities" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The number of fatalities that had occurred at the provider  as of the date this license was issued.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfInjuries" type="EXTENSION-NumberOfInjuries" minOccurs="0">
