          <xs:documentation>The number of injuries that had occurred at this provider as of the date this license was issued.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StateLicensedFacilityCapacity" type="Capacity" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The number of injuries that had occurred at this provider as of the date this license was issued.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="YoungestAgeAuthorizedToServe" type="AgeAuthorizedToServe" minOccurs="0">
        <xs:annotation>
