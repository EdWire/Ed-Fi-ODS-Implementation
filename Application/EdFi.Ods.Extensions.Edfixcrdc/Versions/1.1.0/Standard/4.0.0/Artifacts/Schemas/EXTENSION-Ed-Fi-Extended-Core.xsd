        <ann:TypeGroup>Common</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FiberOpticConnection" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Is the school connected to the internet through fiber-optic connection?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AllClassroomsHaveWiFi" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Does the school have Wi-Fi access in every classroom?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StudentsTakeHomeDevice" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Does the school allow students to take home school-issued devices that can be used to access the internet for student learning?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StudentsBringOwnDevice" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>Does the school allow students to bring to the school student-owned devices that can be used to access the internet for student learning?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WiFiDeviceCount" type="xs:int">
        <xs:annotation>
          <xs:documentation>Number of Wi-Fi enabled devices provided by the school to students for student learning use</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
