<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/4.0.0" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/4.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 4.0.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-ClassGroup">
    <xs:annotation>
      <xs:documentation>A collection of students at a community provider location.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="ClassGroupName" type="EXTENSION-ClassGroupName">
            <xs:annotation>
              <xs:documentation>The name of the class/group of students.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CommunityProviderLocationReference" type="EXTENSION-CommunityProviderLocationReferenceType">
            <xs:annotation>
              <xs:documentation>The community provider location associated with the class group.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialNeedsProvidedIndicator" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>An indication of whether a class or group serves children with special needs.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Capacity" type="Capacity" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The maximum number of students this class group can accomodate at any given time.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DaysAvailablePerWeek" type="EXTENSION-DaysAvailablePerWeek" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of days per week this class/group meets.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="HoursAvailablePerDay" type="EXTENSION-HoursAvailablePerDay" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of hours per day the class/group meets.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ClassGroupMeetingTime" type="EXTENSION-ClassGroupMeetingTime" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The times and days of week this class/group is in session.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Service" type="ServiceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The services provided to students at this class/group.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>ServiceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseReference" type="CourseReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The course being offered by the ClassGroup.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CommunityProviderLocation">
    <xs:annotation>
      <xs:documentation>This entity represents physical locations where services are provided by Community Providers.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="CommunityProviderLocationId" type="EXTENSION-CommunityProviderLocationId">
            <xs:annotation>
              <xs:documentation>A unique number or alphanumeric code assigned to an education organization by a school, school system, a state, or other agency or entity.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CommunityProviderReference" type="CommunityProviderReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the Community Provider associated with the community provider location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AgeRangeDescription" type="EXTENSION-AgeRangeDescription" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An indication of the range of ages of students that this provider location can accommodate.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="YoungestAgeAccepted" type="EXTENSION-AgeAccepted" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The youngest age of students accepted at this location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OldestAgeAccepted" type="EXTENSION-AgeAccepted" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The oldest age of students accepted at this location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Capacity" type="Capacity" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The maximum number of students this provider location can accommodate at any given time.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialNeedsProvidedIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An indication of whether a provider location serves children with special needs.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TimeOpen" type="xs:time" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The time of day this provider location begins providing services.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TimeClose" type="xs:time" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The time of day this provider location stops providing services.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AddressType" type="AddressTypeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The type of address listed for the community provider location.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>AddressTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StreetNumberName" type="StreetNumberName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The street number and street name or post office box number of an address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ApartmentRoomSuiteNumber" type="ApartmentRoomSuiteNumber" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The street number and street name or post office box number of an address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BuildingSiteNumber" type="BuildingSiteNumber" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of the building on the site, if more than one building shares the same address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="City" type="City" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The name of the city in which an address is located.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StateAbbreviation" type="StateAbbreviationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The abbreviation for the state (within the United States) or outlying area in which an address is located.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>StateAbbreviationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="PostalCode" type="PostalCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The five or nine digit zip code or overseas postal code portion of an address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NameOfCounty" type="NameOfCounty" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The name of the county, parish, borough, or comparable unit (within a state) in which an address is located.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CountyFIPSCode" type="CountyFIPSCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Federal Information Processing Standards (FIPS) numeric code for the county issued by the National Institute of Standards and Technology (NIST). Counties are considered to be the 'first-order subdivisions' of each State and statistically equivalent entity, regardless of their local designations (county, parish, borough, etc.). Counties in different States will have the same code. A unique county number is created when combined with the 2-digit FIPS State Code</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InstructionalCalendar" type="EXTENSION-InstructionalCalendar" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies the span of time a community provider is open to provide instruction to students who are under the direction and guidance of teachers and/or administration.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ProgramExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Program">
        <xs:sequence>
          <xs:element name="ProgramCost" type="EXTENSION-ProgramCostDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Defines the cost of a program that is offered by the Education Organization</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ProgramCostDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-RestraintEventExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="RestraintEvent">
        <xs:sequence>
          <xs:element name="RestraintCategory" type="EXTENSION-RestraintCategoryDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
